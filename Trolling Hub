-- Funktion für den Be Normal-Button
BeNormal.MouseButton1Click:Connect(function()
    -- Sendet eine Benachrichtigung an den Spieler
    game:GetService("StarterGui"):SetCore("SendNotification", {
        Title = "Reset Character",
        Text = "Do it yourself",
        Button1 = "Sorry",
        Duration = 25
    })
end)


