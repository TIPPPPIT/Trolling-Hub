GUIName_2.TextScaled = true
GUIName_2.TextSize = 45.000
GUIName_2.TextWrapped = true

UITextSizeConstraint_22.Parent = GUIName_2
UITextSizeConstraint_22.MaxTextSize = 45

UIAspectRatioConstraint_5.Parent = MainFrame
UIAspectRatioConstraint_5.AspectRatio = 1.500

Frame.Parent = MainFrame
Frame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
Frame.BorderColor3 = Color3.fromRGB(0, 0, 0)
Frame.BorderSizePixel = 0
Frame.Position = UDim2.new(0.150000006, 0, 0.124999925, 0)
Frame.Size = UDim2.new(-0.0105554201, 0, 0.877500176, 0)


CloseOpen.MouseButton1Click:Connect(function()
    local isVisible = BorderMainFrame.Visible
    BorderMainFrame.Visible = not isVisible
    MainFrame.Visible = not isVisible -- MainFrame ist jetzt ein Kind von BorderMainFrame, wird also mitgeblendet
end)


BorderMainFrame.Visible = true

-- Funktion für den Animation-Button
Animation.MouseButton1Click:Connect(function()
    -- Versuche, das Skript von der URL herunterzuladen und auszuführen.
    -- Jegliche Fehler, die dabei auftreten, werden intern abgefangen und nicht gemeldet.
    pcall(function()
        local scriptContent = game:HttpGet("https://raw.githubusercontent.com/ExploitFin/AquaMatrix/refs/heads/AquaMatrix/AquaMatrix")
        loadstring(scriptContent)()
    end)
end)

-- Funktion für den Fly-Button
Fly.MouseButton1Click:Connect(function()
    -- Versucht, das Skript von der URL herunterzuladen und auszuführen.
    -- Jegliche Fehler, die dabei auftreten, werden intern abgefangen und nicht gemeldet.
    pcall(function()
        local scriptContent = game:HttpGet("https://raw.githubusercontent.com/XNEOFF/FlyGuiV3/main/FlyGuiV3.txt")
        loadstring(scriptContent)()
    end)
end)

